{
    "routes": [
        {
            "src": "/(.*).html",
            "dest": "/index.html"
        },
        {
            "src": "/(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "/(.*).css",
            "dest": "/$1.css"
        },
        {
            "src": "/(.*).(jpg|jpeg|png|svg|ico|gif)",
            "dest": "/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}